{
	@EFFECTS
	{
		@running_closed
		{
			@AUDIO
			{
				@clip = SpaceY/SpaceYLifters/Sounds/sls_loop_deep // sound_rocket_spurts
			}
		}
		@engage
		{
			@AUDIO
			{
				@clip = SpaceY/SpaceYLifters/Sounds/sls_start // sound_vent_soft
			}
		}
	}
}
